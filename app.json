{
  "expo": {
  "name": "Your FUTAPP",
  "slug": "footbeer",
  "version": "1.0.1",
    "scheme": "futapp",
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
  "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#00A859"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "1.0.1",
      "bundleIdentifier": "com.bmoreiraribeiro.fut",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "futapp"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#00A859"
      },
      "package": "com.bmoreiraribeiro.fut",
      "versionCode": 2,
      "newArchEnabled": false,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "futapp"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/icone_app.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a757d24b-ca1a-4201-be96-1510522a05ba"
      }
    },
    "owner": "brrrocket",
    "plugins": [
      "expo-router",
      "expo-web-browser"
    ]
  }
}
