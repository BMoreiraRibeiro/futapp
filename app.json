{
  "expo": {
  "name": "Your FUTAPP",
  "slug": "footbeer",
  "version": "1.0.3",
    "scheme": "futapp",
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
  "icon": "./assets/images/ic_launcher-web.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/favicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#00A859"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
  "buildNumber": "1.0.3",
      "bundleIdentifier": "com.bmoreiraribeiro.fut",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "futapp"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ic_launcher_foreground.png",
        "backgroundColor": "#00A859"
      },
      "package": "com.bmoreiraribeiro.fut",
  "versionCode": 4,
      "newArchEnabled": false,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "futapp"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/android/ic_launcher-web.png",
      "name": "FutBeer",
      "shortName": "FutBeer",
      "description": "App de gest√£o de futebol com amigos",
      "themeColor": "#00A859",
      "backgroundColor": "#00A859",
      "display": "standalone",
      "startUrl": "/",
      "orientation": "portrait",
      "scope": "/",
      "lang": "pt-PT",
      "dir": "ltr",
      "relatedApplications": [
        {
          "platform": "play",
          "url": "https://play.google.com/store/apps/details?id=com.bmoreiraribeiro.fut",
          "id": "com.bmoreiraribeiro.fut"
        }
      ],
      "preferRelatedApplications": false
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a757d24b-ca1a-4201-be96-1510522a05ba"
      }
    },
    "owner": "brrrocket",
    "plugins": [
      "expo-router",
      "expo-web-browser"
    ]
  }
}
